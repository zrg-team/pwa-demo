{
  "name": "649-pwa",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://zrg-team.github.io/pwa-demo",
  "dependencies": {
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "cra-append-sw": "^2.6.1",
    "i18n-js": "^3.3.0",
    "moment": "^2.24.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-notification-system": "^0.2.17",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "source-map-explorer": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build-all": "GENERATE_SOURCEMAP=false react-app-rewired build  && cra-append-sw --skip-compile ./src/firebaseMessagingWorker.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build-css": "node-sass-chokidar src/assets/sass -o src/assets/styles --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/sass -o src/assets/styles --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "node-sass-chokidar": "^1.3.5",
    "react-app-rewired": "^2.1.3"
  }
}
